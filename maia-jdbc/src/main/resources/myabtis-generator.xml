<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- <classPathEntry
		location="/Program Files/IBM/SQLLIB/java/db2java.zip" /> -->

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<property name="" value=""/>
	
		<commentGenerator>
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>
		
		<jdbcConnection
			driverClass="com.mysql.cj.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/cms?serverTimezone=GMT%2B8" userId="dev" password="123456">
		</jdbcConnection>

		<javaModelGenerator targetPackage="io.vilya.maia.jdbc.entity"
			targetProject="maia-jdbc">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="io.vilya.maia.jdbc.mapper.xml"
			targetProject="maia-jdbc">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="io.vilya.maia.jdbc.mapper" targetProject="maia-jdbc">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="LOGIN_INFO" domainObjectName="LoginInfo">
		</table>

	</context>
</generatorConfiguration>